#include<iostream>
using namespace std;


int main(){
      int M, N;
    cin>>M>>N;
    int arr[M][N];
    for(int i = 0; i < M; i++){
        for(int j = 0; j < N; j++ ){
            cin>>arr[i][j];
        }
    }
    int j = 0;
    while(j < N){
    	int i = 0;
    	int N1 = 0;
    	for(i=0; i <M ; i++){
        	N1 = N1 +arr[i][j];
    	}
    	j++;
    	cout<< N1 << " ";
	}
 

  
}
